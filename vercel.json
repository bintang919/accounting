{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.6.0",
      "config": {
        "phpVersion": "8.2",
        "installDependencies": [
          "libssl-dev"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ]
}